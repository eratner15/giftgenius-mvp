{
  "version": 2,
  "builds": [
    {
      "src": "backend/server-production.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server-production.js"
    },
    {
      "src": "/(.*)",
    /frontend/public/index.html"dest": "/frontend/public/$1"
    }
  ]
}
